{
  "name": "@radum/eslint-config",
  "version": "7.1.0",
  "description": "@radum ESLint Shareable Config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Radu Micu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:radum/eslint-config.git"
  },
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "main": "index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "rules/**/*.js",
    "avajs.js",
    "index.js",
    "jest.js",
    "jsdoc.js",
    "node.js",
    "react.js"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:watch": "ava --watch",
    "lint:js": "eslint --cache --cache-location './.cache/eslint' --ignore-pattern 'tests/mocks' '**/*.js'"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "eslint-plugin-ava": "^15.0.1",
    "eslint-plugin-compat": "^6.0.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.2.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-unicorn": "^55.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^11.2.1",
    "@auto-it/first-time-contributor": "^11.2.1",
    "@auto-it/npm": "^11.2.1",
    "@auto-it/released": "^11.2.1",
    "@types/eslint": "^9.6.1",
    "auto": "^11.2.1",
    "ava": "^6.1.3",
    "eslint": "^9.10.0",
    "nyc": "^17.0.0"
  }
}
