{
  "name": "@radum/eslint-config",
  "version": "4.0.0",
  "description": "@radum ESLint Shareable Config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Radu Micu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:radum/eslint-config.git"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "files": [
    "rules/**/*.js",
    "avajs.js",
    "jest.js",
    "node.js",
    "react.js",
    "index.js"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:watch": "ava --watch",
    "lint:js": "eslint --cache --cache-location './.cache/eslint' --ignore-pattern 'tests/mocks' --ignore-pattern 'ava.config.js' '**/*.js'",
    "release": "np --no-cleanup --no-yarn",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-compat": "^3.6.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-unicorn": "^22.0.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "ava": "^3.8.2",
    "eslint": "^7.0.0",
    "np": "^6.2.3",
    "nyc": "^15.0.1",
    "semantic-release": "^17.3.0"
  }
}
