{
  "name": "@radum/eslint-config",
  "version": "6.1.0",
  "description": "@radum ESLint Shareable Config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Radu Micu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:radum/eslint-config.git"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "files": [
    "rules/**/*.js",
    "avajs.js",
    "jest.js",
    "node.js",
    "react.js",
    "index.js"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:watch": "ava --watch",
    "lint:js": "eslint --cache --cache-location './.cache/eslint' --ignore-pattern 'tests/mocks' --ignore-pattern 'ava.config.js' '**/*.js'",
    "release": "np --no-cleanup --no-yarn",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint-plugin-ava": "^13.0.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.1.0",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-unicorn": "^37.0.1"
  },
  "peerDependencies": {
    "eslint": "^7.32.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "np": "^7.5.0",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.0"
  }
}
