{
  "name": "@radum/eslint-config",
  "version": "7.1.0",
  "description": "@radum ESLint Shareable Config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Radu Micu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:radum/eslint-config.git"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "files": [
    "rules/**/*.js",
    "avajs.js",
    "index.js",
    "jest.js",
    "jsdoc.js",
    "node.js",
    "react.js"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:watch": "ava --watch",
    "lint:js": "eslint --cache --cache-location './.cache/eslint' --ignore-pattern 'tests/mocks' --ignore-pattern 'ava.config.mjs' '**/*.js'"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-form-fields": "^1.2.12",
    "eslint-plugin-react-hook-form": "^0.2.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.13.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^10.36.5",
    "@auto-it/first-time-contributor": "^10.36.5",
    "@auto-it/npm": "^10.36.5",
    "@auto-it/released": "^10.36.5",
    "auto": "^10.36.5",
    "ava": "^4.2.0",
    "eslint": "^8.13.0",
    "nyc": "^15.1.0"
  }
}
